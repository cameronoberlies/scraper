{
    "manifest_version": 3,
    "name": "Marketplace Vehicle Scraper",
    "version": "1.0",
    "description": "Scrapes vehicle data from Facebook Marketplace",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "*://*.facebook.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}